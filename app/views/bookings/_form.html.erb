      <%= simple_form_for [character, booking] do |f| %>
        <%= f.input :start_date,
          as: :string,
          input_html: { data: { controller: "datepicker" } } %>
        <%= f.input :end_date,
          as: :string,
          input_html: { data: { controller: "datepicker" } } %>
        <%= f.submit 'Book a hero' %>
      <% end %>
